{
  "name": "mxvoice",
  "productName": "Mx. Voice",
  "version": "3.1.5",
  "description": "Improv Audio Software",
  "repository": "https://github.com/minter/mxvoice-electron",
  "main": "src/index.js",
  "keywords": [],
  "author": {
    "name": "H. Wade Minter",
    "email": "wade@wademinter.com"
  },
  "scripts": {
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "release": "electron-builder --publish always",
    "postinstall": "electron-builder install-app-deps",
    "start": "electron ."
  },
  "build": {
    "appId": "com.electron.mx.-voice",
    "productName": "Mx. Voice",
    "copyright": "Copyright Â© 2021 Wade Minter and Andrew Berkowitz",
    "afterSign": "electron-builder-notarize",
    "win": {
      "icon": "src/assets/icons/mxvoice.ico"
    },
    "mac": {
      "category": "public.app-category.music",
      "icon": "src/assets/icons/mxvoice",
      "darkModeSupport": true
    },
    "dmg": {
      "icon": "src/assets/icons/dmg-image.icns",
      "background": "src/assets/images/dmg-background-image.png",
      "contents": [
        {
          "x": 180,
          "y": 340
        },
        {
          "x": 450,
          "y": 340,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "minter",
        "repo": "mxvoice-electron"
      }
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@octokit/rest": "^18.12.0",
    "animate.css": "^4.1.1",
    "axios": "^0.21.4",
    "better-sqlite3": "^7.4.3",
    "bootstrap": "^4.6.0",
    "electron-log": "^4.4.1",
    "electron-prompt": "^1.7.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "electron-updater": "^4.3.9",
    "electron-util": "^0.17.0",
    "howler": "^2.2.3",
    "jquery": "^3.6.0",
    "jquery-contextmenu": "^2.9.2",
    "lodash": "^4.17.21",
    "markdown-it": "^12.2.0",
    "mousetrap": "^1.6.5",
    "music-metadata": "^7.11.3",
    "n-readlines": "^1.0.1",
    "popper.js": "^1.14.4",
    "uuid": "^8.3.2",
    "wavesurfer.js": "^5.2.0"
  },
  "devDependencies": {
    "electron": "22.0.1",
    "electron-builder": "^22.11.7",
    "electron-builder-notarize": "^1.2.0",
    "electron-reload": "^1.5.0"
  }
}
